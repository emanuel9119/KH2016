<local:Child_Window x:Class="VN_Editor_KH16.FlowEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VN_Editor_KH16"
        mc:Ignorable="d"
        Title="Flow Editor" Height="300" Width="300"
        Closing="Window_Closing"
        Activated="Window_Active"
        Deactivated="Window_Deactive"
        LocationChanged="Window_Moved">
    <Grid RenderTransformOrigin="0.417,0.279">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button Content="Add to window" Click="Window_Dropped" Margin="0,0,-0.4,0" Height="58" VerticalAlignment="Top"/>
        <Canvas Name="FlowCanv" Margin="0,57,0,0" ></Canvas>
    </Grid>
</local:Child_Window>
